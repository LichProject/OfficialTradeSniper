<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>LiveSearchEngine</Title>
        <Description>Live searching library for path of exile.</Description>
        <Copyright>Copyright Â© LichProject 2021</Copyright>
        <RepositoryUrl>https://github.com/LichProject/OfficialTradeSniper</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageProjectUrl>https://github.com/LichProject/OfficialTradeSniper</PackageProjectUrl>
        <PackageTags>sniper pathofexile trade poe</PackageTags>
        <PackageVersion>1.0.4</PackageVersion>
        <PackageLicenseUrl>https://licenses.nuget.org/MIT</PackageLicenseUrl>
