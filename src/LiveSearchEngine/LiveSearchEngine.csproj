    </ItemGroup>

    <ItemGroup>
      <Reference Include="websocket-sharp, Version=1.0.2.59611, Culture=neutral, PublicKeyToken=5660b08a1845a91e">
        <HintPath>..\..\libs\websocket-sharp.dll</HintPath>
      </Reference>
    </ItemGroup>
    
